{
  "type": "module",
  "name": "container-updater",
  "version": "1.0.1",
  "description": "A service to update containers in a docker swarm installed in the same machine",
  "main": "dist/app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --experimental-loader ts-node/esm --test \"test/**/*.test.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "concurrently \"tsc --watch\" \"node --watch dist/app.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/jwt": "^9.0.1",
    "@fastify/sensible": "^6.0.1",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "c8": "^10.1.2",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "fastify-tsconfig": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
